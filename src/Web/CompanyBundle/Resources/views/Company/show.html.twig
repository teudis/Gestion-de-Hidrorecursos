
{% extends "::frontend.html.twig" %}

{% block id 'usuario' %}
{% block title %}Datos Empresa{% endblock %}
{% block nav %}
{% include ':includes:destacada.html.twig' %}
{% endblock%}
{% block article %}
<div class="portada">
    <div class="navbar">
        <div class="navbar-inner">
            <ul class="nav">
                <li><a href="{{ path('WebCompanyBundle_listado_company') }}"><i class="icon-list"></i> View List</a></li>
                <li><a href="{{ path('WebCompanyBundle_editar_company', { 'id': entity.id }) }}"><i class="icon-edit"></i>Edit Company</a></li>
                <li><a href="{{ path('WebCompanyBundle_delete_company', { 'id': entity.id }) }}">
                        <i class="icon-remove"></i>Delete
                    </a>
                </li>
            </ul>
        </div>
    </div>
<div class="row-fluid">
        <div class="span2"><div class="pull-right"><strong>{% trans %}Nombre{% endtrans %}:</strong></div></div>
        <div class="span10">{{ entity.name }}</div>
   </div>
  
   <div class="row-fluid">
        <div class="span2"><div ><strong>{% trans %}Descripcion{% endtrans %}:</strong></div></div>
        <div class="span10">{{ entity.description }}</div>
   </div>
   
   {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
   <p>
     <div class="row-fluid">
        <div class="span2"><div class="pull-right"><strong>{% trans %}Pais{% endtrans %}:</strong></div></div>
        <div class="span10">{{ entity.country }}</div>
   </div>
   <div class="row-fluid">
        <div class="span2"><div class="pull-right"><strong>{% trans %}Email{% endtrans %}:</strong></div></div>
        <div class="span10">{{ entity.email }}</div>
   </div>
    
       
    <div class="row-fluid">
        <div class="span2"><div class="pull-right"><strong>{% trans %}Web Site{% endtrans %}</strong></div></div>
        <div class="span10">{{ entity.website }}</div>
   </div>
   </p>
  
   
   {% endif %}
   
    
</div>
{% endblock %}